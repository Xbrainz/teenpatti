{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":  [2, { "allow": ["_id"] }],
        "indent": [2, 4],
        "no-console": 0,
        "no-param-reassign": [2, { "props": false }]
    }
}
